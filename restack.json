{
  "name": "instrument-api",
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "runtime": "docker",
  "services": [
    {
      "name": "api",
      "port": 8000,
      "command": [
        "uvicorn",
        "app.main:app",
        "--host",
        "0.0.0.0",
        "--port",
        "8000"
      ]
    }
  ]
}
